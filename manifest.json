{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"YaTQA",
		"version":"1.0.1",
		"minimum-overwolf-version": "0.77.1.0",
		"author":"JN-GAMES Software",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":""
	},
	"data": {
		"start_window":"index",
		"windows":{
			"index":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":212, "height":800},
					"min_size":{"width":212, "height":800},
					"max_size":{"width":212, "height":1000}
				},
			"welcome":{
					"file":"Files/welcome.html",
					"transparent":false,
					"resizable":false,
					"size":{"width":720, "height":512},
					"min_size":{"width":720, "height":512},
					"max_size":{"width":720, "height":512}
				}
		},
		"externally_connectable": {
			"matches": [
				"http://www.youtube.com",
				"https://www.youtube.com"
			]
		},
		"extra-objects": {
			"teamspeak-plugin": {
				"file": "Files/teamspeak-plugin.dll",
				"class": "overwolf.plugins.TeamSpeakPlugin"
			}
		}
	},
	"permissions": [
		"GameInfo", "Extensions"
	]
}